<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.iot_smarthome">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UserMainActivity"/>
        <activity android:name=".User_Light_Activity"/>
        <activity android:name=".User_TemHum_Activity"/>
        <activity android:name=".User_Fire_Activity"/>
        <receiver android:name=".AlarmReceiver"/>
        <service android:name=".Servicee" android:enabled="true"/>
        <service android:name=".Service1" android:enabled="true"/>
    </application>

</manifest>